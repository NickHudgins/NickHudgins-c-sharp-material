
@{
    ViewData["Title"] = "Hello";

    int result = 7;
}

<h2>Hello</h2>
<h3>This is the Hello view</h3>

<h1> @result </h1>


<ul>
    @for (int i = 0; i < 100; i++)
    {
        if (i < 50)
        {
            <li styl="color:red"> @(i + 1)  </li>
        }
        else
        {
            <li styl="color: blue"> @(i + 1)  </li>
        }
    }
</ul>

